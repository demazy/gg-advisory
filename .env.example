env:
  OPENAI_API_KEY: ${{ secrets.OPENAI_API_KEY }}
  MODE: backfill-months
  START_YM: "2025-02"
  END_YM:   "2025-02"

  # Intake tuners (more permissive for backfill tests):
  ITEMS_PER_SECTION: "6"
  MIN_TEXT_CHARS: "350"        # was 500
  PER_DOMAIN_CAP: "3"
  MIN_TOTAL_ITEMS: "2"         # allow digest if 2+ items pass
  PRIORITY_MIN_CHARS: "250"
  PRIORITY_DOMAINS: "aemo.com.au,arena.gov.au,cefc.com.au,ifrs.org,efrag.org,dcceew.gov.au,ec.europa.eu"  
  TEMP: "0.2"
  MODEL: gpt-4o-mini

  # PDF hybrid stays on:
  MAX_PDF_BYTES: "5242880"
  PDF_TRUSTED: "aemo.com.au,ifrs.org,efrag.org,dcceew.gov.au,arena.gov.au,cefc.com.au,irena.org"

  # Debug logs to see drop reasons:
  DEBUG: "1"
